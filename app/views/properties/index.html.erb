<div class="container-fluid">
        <main role="main">
        
          <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3 border-bottom">
            <h1 class="h2">Properties list of</h1>
            
          </div>
          <div class="table">
            <table class="table table-striped table-sm">
              <thead>
                <tr>
                  <th>id</th>
                  <th>Name of the properties</th>
                  <th>Posted By </th>
                  <th>Show</th>
                  <th>Edit</th>
                  <th>Destroy</th>
                </tr>
              </thead>
              <tbody>
              <% @properties.each do |pt| %>
              <%if pt.agent_id==current_agent.id%>
                <tr>
                <th scope="row"><%= pt.id%></th>
                  <td><%= pt.name %></td><>
                  <td><%= pt.agent.name%></td>
                 <td> <%= link_to 'show', property_path(pt) , style: "text-decoration: none;"%> </td>
                 <td> <%= link_to 'edit', edit_property_path(pt), style: "text-decoration: none;" %> </td>
                 <td><%= button_to 'remove',property_path(pt),method:'delete',data:"are you confirm" %></td>
                </tr>
                <%end%>
                <%end%>
              </tbody>
            </table>
          </div>
        </main>
      </div>
    </div>







  
    
